{{#>base/base title="Richiesta assistenza - Finalizzare procedura"}}
<main>
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-12 col-lg-10">
        {{> cmp-breadcrumbs/cmp-breadcrumbs link1="Home" link2="Servizi" link3="Prenotazioni"}}
        <div class="mb-3 mb-lg-3">
          {{>cmp-heading/cmp-heading
          title="Richiesta inviata"
          btnClass="mt-3"
          subTitle="La richiesta di assistenza è stata inviata con successo, sarai ricontattato presto. </b></p>
          <p class='subtitle-small pt-3 pt-lg-4 d-lg-flex flex-column'>Abbiamo inviato il riepilogo all’email:
            <strong>giulia.bianchi@gmail.com</strong>"
            iconTitle="it-check-circle"
            iconClass="mr-10 icon-sm mb-1"
            sm=true
            }}
        </div>
      </div>
    </div>
  </div>

  <div class="bg-primary">
    <div class="container">
      <div class="row d-flex justify-content-center bg-primary">
        <div class="col-12 col-lg-6 p-lg-0 px-3">
          {{>cmp-rating/cmp-rating id-title="rating"}}
        </div>
      </div>
    </div>
  </div>

  <div class="bg-grey-card shadow-contacts">
    <div class="container">
      <div class="row">
        <div class="col-12 col-lg-6 offset-lg-3 p-contacts">
          {{>cmp-contacts/cmp-contacts}}
        </div>
      </div>
    </div>
  </div>
</main>
{{/base/base }}